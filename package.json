{
  "name": "@hemjs/http",
  "version": "0.1.3",
  "description": "Utility classes and constants for use with HTTP messages",
  "author": "Augustus Kamau",
  "license": "MIT",
  "keywords": [
    "hemjs",
    "http",
    "utils"
  ],
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc -p tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "lint": "eslint \"{src,tests}/**/*.ts\" --fix",
    "test": "jest",
    "test:cov": "jest --coverage",
    "prerelease": "npm run prebuild && npm run build",
    "release": "release-it"
  },
  "devDependencies": {
    "@commitlint/cli": "17.6.3",
    "@commitlint/config-conventional": "17.6.3",
    "@types/jest": "29.5.1",
    "@types/node": "18.16.15",
    "@typescript-eslint/eslint-plugin": "5.59.7",
    "@typescript-eslint/parser": "5.59.7",
    "eslint": "8.41.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-import": "2.27.5",
    "husky": "8.0.3",
    "jest": "29.5.0",
    "lint-staged": "13.2.2",
    "prettier": "2.8.8",
    "release-it": "15.10.3",
    "rimraf": "5.0.1",
    "ts-jest": "29.1.0",
    "typescript": "5.0.4"
  },
  "homepage": "https://github.com/hemjs/http#readme",
  "bugs": {
    "url": "https://github.com/hemjs/http/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hemjs/http.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "eslint --fix"
    ]
  }
}
